{
  "Version": "1.0.0",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "ApiServer": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Properties": {
      "ApplicationName": "ApiServer"
    }
  },
  "ConnectionStrings": {
    "ApiContext": " Data Source=ApiServer.db;Cache=Shared"
  }
}
